@using Scentasy.Models.Models
@{
    ViewBag.Title = "All Products";
    Layout = "~/Views/Shared/_ClientLayout.cshtml";
}
<div class="content_breadcum"></div>
<div class="container">
    <ul class="breadcrumb">
        <li><a href="index9328.html?route=common/home"><i class="fa fa-home"></i></a></li>
        <li><a href="index5b66.html?route=product/category&amp;path=20&amp;sort=pd.name&amp;order=DESC">Floral</a></li>
        <li><a href="indexba17.html?route=product/category&amp;path=20_33&amp;sort=pd.name&amp;order=DESC">Citrus</a></li>
        <li><a href="indexcee4.html?route=product/category&amp;path=20_33_63">Giorgio Armani</a></li>
    </ul>
    <div class="row">
        <aside id="column-left" class="col-sm-3 hidden-xs">
            <div class="box">
                <div class="box-heading"><span>Categories</span></div>
                <div class="box-content ">
                    <ul class="box-category treeview-list treeview">
                        <li>
                            <a href="~/Client/Men">Men</a>
                        </li>



                        <li>
                            <a href="~/Client/Women">Women</a>
                        </li>

                        <li>
                            <a href="~/Client/All">All</a>
                        </li>

                    </ul>
                </div>
            </div>
            <div class="box container" id="latest">
                <div class="box-heading"><span>Latest</span></div>
                <div class="box-content">
                    <div class="box-product productbox-grid" id="latest-grid">
                        @foreach (var i in ViewData["latest"] as List<ProductViewModel>)
                        {

                            <div class="product-items">
                                <div class="product-block product-thumb transition">
                                    <div class="product-block-inner">
                                        <div class="image">
                                            <a href="~/Client/ViewPerfume/@i.Id">
                                                <img src="~/image/product/@i.Photo" alt="White Diamonds" title="White Diamonds" class="img-responsive" />
                                            </a>
                                            <div class="product_hover">
                                                <div class="hover_button">
                                                    <button class="wishlist_button" type="button" data-toggle="tooltip" title="Add to WishList" onclick="wishlist.add('49');"><i class="fa fa-heart"></i></button>
                                                    <button class="compare_button" type="button" data-toggle="tooltip" title="Add to Compare" onclick="compare.add('49');"><i class="fa fa-area-chart"></i></button>
                                                    <div class="quickview" data-placement="left" title="Quick View" data-toggle="tooltip"><a href="_b_Notice_/b__%20Undefined%20index_%20quick%20in%20_b_/var/www/html/OPC09/OPC090218/catalog/view/theme/OPC090218/template/extension/module/latest.tpl_/b_%20on%20line%20_b_3/b_.e7.delaye"><i class="fa fa-eye"></i></a></div>
                                                </div>
                                                <div class="rating">
                                                    <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                                    <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                                    <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                                    <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                                    <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="caption">
                                            <h4><a href="~/Client/ViewPerfume/@i.Id">@i.Name</a></h4>
                                            <p class="price">
                                                <span class="price-new">@i.Price</span>
                                                <span class="price-tax">Ex Tax: $90.00</span>
                                            </p>
                                            <div class="button_cart">
                                                <button type="button" onclick="cart.add('49');">Add to Cart</button>
                                                <div class="quickview" data-toggle="tooltip"><a href="_b_Notice_/b__%20Undefined%20index_%20quick%20in%20_b_/var/www/html/OPC09/OPC090218/catalog/view/theme/OPC090218/template/extension/module/latest.tpl_/b_%20on%20line%20_b_6/b_.e8.delaye"><i class="fa fa-eye"></i></a></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        }

                        
]
                    </div>
                </div>
            </div>
            <span class="latest_default_width" style="display:none; visibility:hidden"></span>
            <div id="banner0" class="single-banner">
                <div class="item">
                    <a href="#"><img src="~/Content/image/cache/catalog/left-banner-252x350.jpg" alt="LeftBanner" class="img-responsive" /></a>
                </div>
            </div>

        </aside>
        <div id="content" class="col-sm-9">
            <h1 class="page-title">Giorgio Armani</h1>
            <div class="category_filter">
                <div class="col-md-4 btn-list-grid">
                    <div class="btn-group">
                        <button type="button" id="grid-view" class="btn btn-default grid active" data-toggle="tooltip" title="Grid"></button>
                        <button type="button" id="list-view" class="btn btn-default list" data-toggle="tooltip" title="List"></button>
                    </div>
                </div>
                <div class="compare-total"><a href="index6431.html?route=product/compare" id="compare-total">Product Compare (0)</a></div>
                <div class="pagination-right">
                </div>
            </div>
            <div class="row">

                @foreach (var i in ViewData["all"] as List<Perfume>)
                {
                    <div class="product-layout product-list col-xs-12">
                        <div class="product-block product-thumb">
                            <div class="product-block-inner">
                                <div class="image">
                                    <a href="~/image/product/@i.Image"><img src="~/image/product/@i.Image" alt="Acqua Di Gio" title="Acqua Di Gio" class="img-responsive" /></a>
                                    <div class="saleback">
                                        <span class="saleicon sale">Sale</span>
                                    </div>
                                    <div class="product_hover grid">
                                  
                                        <div class="rating">
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="product-details">
                                    <div class="caption">
                                        <h4><a href="~Client/ViewPerfume/@i.Id">@i.Name</a></h4>
                                        <div class="rating list">
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i></span>
                                            <span class="fa fa-stack"><i class="fa fa-star fa-stack-1x"></i></span>
                                        </div>
                                        <p class="desc">
                                            Canon's press material for the EOS 5D states that it 'defines (a) new D-SLR category', while we'r..
                                        </p>
                                        <p class="price">
                                            <span class="price-new">$@i.Price</span>
                                            <span class="price-tax">Ex Tax: $80.00</span>
                                        </p>
                                        <div class="button_cart">
                                            <button type="button" onclick="cart.add('30');">Add to Cart</button>
                                            <div class="quickview" type="button" data-toggle="tooltip"><a href="~/Client/ViewPerfume/@i.Id"><i class="fa fa-eye"></i></a></div>
                                        </div>
                                    </div>
                                    <div class="button-group">
                                        <button type="button" class="wishlist" title="Add to WishList" onclick="wishlist.add('30');"><i class="fa fa-heart"></i>Add to WishList</button>
                                        <span class="separator">|</span>
                                        <button type="button" class="compare" title="Add to Compare" onclick="compare.add('30');"><i class="fa fa-area-chart"></i>Add to Compare</button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                }


                
            </div>
          
        </div>
    </div>
</div>